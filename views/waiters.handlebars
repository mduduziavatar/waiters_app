<div class="row">
    <form class="main" action="/waiters:" method="get">

        <h1>Names</h1>
        {{#each name}}
        <h2><a href="/waiters/{{this.name}}">{{this.name}}</a></h2>
        {{/each}}
        <h5>Please select days below:</h5>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="Monday" name="Monday">
            <label class="form-check-label" for="inlineCheckbox1">Monday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="Tuesday" name="Tuesday">
            <label class="form-check-label" for="inlineCheckbox2">Tuesday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Wednesday" name="Wednesday">
            <label class="form-check-label" for="inlineCheckbox3">Wednesday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="Thursday" name="Thursday">
            <label class="form-check-label" for="inlineCheckbox1">Thursday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="Friday" name="Friday">
            <label class="form-check-label" for="inlineCheckbox2">Friday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Saturday" name="Saturday">
            <label class="form-check-label" for="inlineCheckbox3">Saturday</label>
        </div>
        <div class="form-check form-check-inline">
            <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Sunday" name="Sunday">
            <label class="form-check-label" for="inlineCheckbox3">Sunday</label>
        </div><button type="submit" class="userBtn">Home</button>
    </form>
</div>